<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobilenavigation"
    app:startDestination="@+id/navigation_home">

    <fragment android:id="@+id/navigation_home"
        android:name="com.example.vims_user.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment android:id="@+id/navigation_about"
        android:name="com.example.vims_user.ui.about.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about"/>

    <fragment android:id="@+id/navigation_notice"
        android:name="com.example.vims_user.ui.notice.NoticeFragment"
        android:label="Notice"
        tools:layout="@layout/fragment_notice"/>

    <fragment android:id="@+id/navigation_faculity"
        android:name="com.example.vims_user.ui.faculity.FaculityFragment"
        android:label="Faculity"
        tools:layout="@layout/fragment_faculity"/>


    <fragment android:id="@+id/navigation_gallery"
        android:name="com.example.vims_user.ui.gallery.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery"/>
